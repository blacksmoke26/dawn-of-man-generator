<?xml version="1.0" encoding="utf-8"?>
<environment>
	<resource_factor value="0.6"/>
	<ford_distance_factor value="1.5"/>
	<sun_angle_factor value="0.8"/>
	<trees values="Fir Pine Spruce Rye Einkorn Emmer Flax BitterVetch Chickpeas Lentils Peas Blackberry Blueberry Raspberry Strawberry Chestnut Pear Cherry Service"/>
	<tree_override_prototypes>
		<tree_override_prototype>
			<id value="Fir"/>
			<min_altitude value="0"/>
			<max_altitude value="14"/>
		</tree_override_prototype>
		<tree_override_prototype>
			<id value="Spruce"/>
			<min_altitude value="0"/>
			<max_altitude value="14"/>
		</tree_override_prototype>
		<tree_override_prototype>
			<id value="Pine"/>
			<min_altitude value="2"/>
			<max_altitude value="16"/>
		</tree_override_prototype>
	</tree_override_prototypes>
	<seasons>
		<season id="Spring" setup_id="Spring" duration="0.2" precipitation_chance="0.25" windy_chance="0.5" very_windy_chance="0.1" fish_boost="0.25">
			<min_temperature value="5"/>
			<max_temperature value="25"/>
		</season>
		<season id="Summer" setup_id="Summer" duration="0.2" precipitation_chance="0.0" windy_chance="0.25">
			<min_temperature value="20"/>
			<max_temperature value="35"/>
		</season>
		<season id="Fall" setup_id="Fall" duration="0.2" precipitation_chance="0.25" windy_chance="0.5" very_windy_chance="0.1">
			<min_temperature value="5"/>
			<max_temperature value="25"/>
		</season>
		<season id="Winter" setup_id="Winter" snow_setup_id="WinterSnow" duration="0.4" precipitation_chance="0.4" windy_chance="0.5" very_windy_chance="0.5" reduced_fish="true" reduced_fauna="true">
			<min_temperature value="-15"/>
			<max_temperature value="10.0"/>
		</season>
	</seasons>
</environment>
